      </complexType>
      -->
      <element name="bar" type="tns:bar"/>
      <element name="barResponse" type="tns:barResponse"/>
      <element name="foo" type="tns:bar"/>
      <element name="fooResponse" type="tns:barResponse"/>
    </schema>
  </types>
  <message name="ActionEndpoint_bar">
    <part name="parameters" element="tns:bar"/>
  </message>
  <message name="ActionEndpoint_barResponse">
    <part name="result" element="tns:barResponse"/>
  </message>
  <message name="ActionEndpoint_foo">
